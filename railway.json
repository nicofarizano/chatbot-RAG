{
    "services": [
      {
        "name": "ollama",
        "source": {
          "type": "image",
          "image": "nicofarizano/ollama-preloaded:llama2"
        },
        "envVars": {
          "PORT": "11434"
        },
        "ports": [
          11434
        ]
      },
      {
        "name": "chatbot",
        "source": {
          "type": "github",
          "repo": "nicofarizano/chatbot-repo",
          "branch": "main",
          "dockerfilePath": "./Dockerfile"
        },
        "envVars": {
          "OLLAMA_HOST": "http://ollama:11434"
        },
        "ports": [
          11435
        ],
        "dependsOn": ["ollama"]
      }
    ]
  }  